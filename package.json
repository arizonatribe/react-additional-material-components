{
  "name": "react-additional-material-components",
  "version": "1.0.6",
  "description": "Some useful React web components (several higher order components) that leverage Material-UI components",
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "BABEL_ENV=production babel src --out-dir dist --ignore node_modules/",
    "clean": "rm -rf ./dist/*",
    "lint": "eslint ./src/.",
    "prepublish": "npm run build",
    "test": "no tests for you"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/arizonatribe/react-additional-material-components.git"
  },
  "keywords": [
    "javascript",
    "react",
    "node",
    "material",
    "materialui",
    "web",
    "components"
  ],
  "author": "David Nunez <arizonatribe@gmail.com>",
  "contributors": [
    "Steve McDowall <steve_mcdowall@mentor.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/arizonatribe/react-additional-material-components#readme",
  "dependencies": {
    "utilitarian": "1.0.5",
    "aphrodite": "1.1.0",
    "material-ui": "0.16.6",
    "react": "15.4.2",
    "react-measure": "1.4.5"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-native-modules": "6.9.3",
    "babel-preset-react": "6.11.1",
    "eslint": "3.5.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-es2015-modules-commonjs"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    },
    "compact": false
  }
}
